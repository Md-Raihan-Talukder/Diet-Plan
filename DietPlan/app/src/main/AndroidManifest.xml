        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.user.dietplan.MainActivity" />
        <activity
            android:name="com.example.user.dietplan.Register"
            android:label="REGISTER" />
        <activity
            android:name="com.example.user.dietplan.Welcome"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.dietplan.Home"
            android:label="HOME" />
