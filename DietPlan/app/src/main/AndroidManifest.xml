        <activity android:name="com.example.user.dietplan.Exercise" />
        <activity android:name="com.example.user.dietplan.Food" />
        <activity android:name="com.example.user.dietplan.ShowNews" />
        <activity android:name="com.example.user.dietplan.Details" />
        <activity android:name="com.example.user.dietplan.ShowTips" />
